{
  "highlight_rules": [
    {
      "rule_type": "exact",
      "pattern": "red",
      "style": "color:red;font-weight:bold;"
    },
    {
      "rule_type": "regex",
      "pattern": "\\b(?:\\d{1,3}\\.){3}\\d{1,3}\\b",
      "style": "color:red;font-weight:bold;"
    }
  ],
  "deletion_rules": [
    {
      "rule_type": "exact",
      "pattern": "DEBUG"
    },
    {
      "rule_type": "regex",
      "pattern": "<\\w> = <>"
    }
  ],
  "change_rules": [
    {
      "rule_type": "exact",
      "pattern": "red",
      "replacement": "blue"
    },
    {
      "rule_type": "regex",
      "pattern": "\\bsigma\\b",
      "replacement": "boy"
    }
  ]
}
